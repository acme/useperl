[% page.title = 'use.perl.org journals of ' _ user.nickname %]

<div class="container">

<div class="row">

<div class="span4">
<h2><a href="[% c.uri_for('~' _ user.nickname _ '/' ) %]">[% user.nickname |
        html %]</a>'s journals</h2>

<ul>
[% FOREACH journal IN journals %]
  <li><a href="[% c.uri_for('~' _ user.nickname _ '/journal/' _
journal.id ) %]">[% journal.description | html %]</a></li>
[% END %]
</ul>

</div> <!-- /span4 -->

</div> <!-- row -->
</div> <!-- /container -->

